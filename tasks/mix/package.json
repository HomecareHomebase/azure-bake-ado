{
  "name": "@azbake/mix",
  "version": "0.1.0",
  "description": "Azure DevOps GUI Task for using Bake Mix @ github.com/HomecareHomebase/azure-bake",
  "main": "dist/index.js",
  "typings": "index.ts",
  "directories": {
    "lib": "dist"
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -w -p .",
    "test": "mocha -r ts-node/register src/tests.ts",
    "clean": "run-script-os",
    "clean:build": "run-script-os",
    "clean:win32": "powershell.exe -Command { Remove-Item ./dist, ./node_modules -Recurse -Force }",
    "clean:build:win32": "powershell.exe -Command { Remove-Item ./dist, ./node_modules -Recurse -Force } && tsc -p ."
  },
  "author": "Tanner Watson",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^11.13.1",
    "@types/q": "^1.5.2",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "mocha": "^6.1.3",
    "nyc": "^14.0.0",
    "typescript": "^3.4.3",
    "ts-node": "^8.1.0",
    "run-script-os": "^1.0.5"
  },
  "dependencies": {
    "azure-pipelines-task-lib": "^2.8.0",
    "docker-common": "file:../../imports/docker-common-1.0.0.tgz",
    "run-script-os": "^1.0.5"
  }
}
