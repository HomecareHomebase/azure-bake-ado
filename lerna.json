{
    "version": "0.1.0",
    "npmClient": "npm",
    "command": {
      "publish": {
        "conventionalCommits": "true",
        "ignoreChanges": [
          "ignored-file",
          "*.md",
          "*.yaml",
          "*.yml"
        ],
        "message": "chore(release): publish"
      }
    },
    "packages": [
      "tasks/*"
    ]
  }
  